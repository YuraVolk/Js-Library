{
  "name": "js-components-library",
  "version": "1.0.0",
  "description": "A library of JavaScript-based components built with Lit, React and Vue.",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "build:lit": "webpack --mode production --config webpack/webpack.config.lit.js",
    "build:vue": "webpack --mode production --config webpack/webpack.config.vue.js",
    "build:react": "webpack --mode production --config webpack/webpack.config.react.js",
    "build": "npm run build:lit && npm run build:vue && npm run build:react",
    "start:lit": "webpack-dev-server --mode development --open --config webpack/webpack.config.lit.js",
    "start:vue": "webpack-dev-server --mode development --open --config webpack/webpack.config.vue.js",
    "start:react": "webpack-dev-server --mode development --open --config webpack/webpack.config.react.js",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix"
  },
  "author": "Yuriy Volkovskiy",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vue/compiler-sfc": "^3.3.11",
    "babel-loader": "^9.1.3",
    "babel-preset-typescript-vue3": "^2.0.17",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.4",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "less-plugin-clean-css": "^1.5.1",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "vue-loader": "^17.3.1",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "babel-core": "^7.0.0-bridge.0",
    "lit": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^3.3.11"
  }
}
